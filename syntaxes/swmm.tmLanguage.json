{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "SWMM files",
  "patterns": [
    
    {
      "include": "#comment"
    },
    {
      "include": "#header"
    },
    {
      "include": "#sections"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#shapes"
    },
    {
      "include": "#options"
    },
    {
      "include": "#numerics"
    },
    {
      "include":"#dates"
    }

  ],
  "repository": {
    "titleBlock":{
      "begin":"[TITLE]",
      "end":"[OPTIONS]",
      "name":"comment.title.swmm"
    },
    "header": {
      "patterns": [
        {
          "begin": "]\\s*",
          "while": "^;;",
          "name":"variable.parameter.header.swmm"
          
        },
        {
          "match": "(?<=;;-{5}).*",
          "name": "variable.parameter.dev.swmm"
        }
      ]
    },
    "comment": {
      "name": "comment.line.semicolon.swmm",
      "begin": ";",
      "end": "\\n"
    },
    "sections": {
      "name": "keyword.section.swmm",
      "match": "^\\[.*(?=\\])"
    },      
    "keywords":{
      "name":"string.keyword.swmm",
      "match":"(?<=\\s)USE(?=\\s)|(?<=\\s)SAVE(?=\\s)|(?<=\\s)TIMESERIES(?=\\s)|(?<=\\s)FILE(?=\\s)|(?<=\\s)INTENSITY(?=\\s)|(?<=\\s)VOLUME(?=\\s)|(?<=\\s)CUMULATIVE(?=\\s)|(?<=\\s)IMPERVIOUS(?=\\s)|(?<=\\s)PERVIOUS(?=\\s)|(?<=\\s)SURFACE(?=\\s)|(?<=\\s)SOIL(?=\\s)|(?<=\\s)PAVEMENT(?=\\s)|(?<=\\s)STORAGE(?=\\s)|(?<=\\s)DRAIN(?=\\s)|(?<=\\s)DRAINMAT(?=\\s)|(?<=\\s)REMOVALS(?=\\s)|(?<=\\s)BC(?=\\s)|(?<=\\s)RG(?=\\s)|(?<=\\s)GR(?=\\s)|(?<=\\s)IT(?=\\s)|(?<=\\s)PP(?=\\s)|(?<=\\s)RB(?=\\s)|(?<=\\s)RD(?=\\s)|(?<=\\s)VS(?=\\s)|(?<=\\s)LATERAL(?=\\s)|(?<=\\s)DEEP(?=\\s)|(?<=\\s)PLOWABLE(?=\\s)|(?<=\\s)IMPERVIOUS(?=\\s)|(?<=\\s)PERVIOUS(?=\\s)|(?<=\\s)REMOVAL(?=\\s)|(?<=\\s)FREE(?=\\s)|(?<=\\s)NORMAL(?=\\s)|(?<=\\s)FIXED(?=\\s)|(?<=\\s)TIDAL(?=\\s)|(?<=\\s)TIMESERIES(?=\\s)|(?<=\\s)OVERFLOW(?=\\s)|(?<=\\s)CUTOFF(?=\\s)|(?<=\\s)TABULAR(?=\\s)|(?<=\\s)WEIR(?=\\s)|(?<=\\s)FUNCTIONAL(?=\\s)|(?<=\\s)SIDE(?=\\s)|(?<=\\s)BOTTOM(?=\\s)|(?<=\\s)YES(?=\\s)|(?<=\\s)NO(?=\\s)|(?<=\\s)TRANSVERSE(?=\\s)|(?<=\\s)SIDEFLOW(?=\\s)|(?<=\\s)V-NOTCH(?=\\s)|(?<=\\s)TRAPEZOIDAL(?=\\s)|(?<=\\s)ROADWAY(?=\\s)|(?<=\\s)OUTLET(?=\\s)|(?<=\\s)HOURLY(?=\\s)|(?<=\\s)MONTHLY(?=\\s)|(?<=\\s)WEEKLY(?=\\s)|(?<=\\s)WEEKEND(?=\\s)|(?<=\\s)POW(?=\\s)|(?<=\\s)EXP(?=\\s)|(?<=\\s)SAT(?=\\s)|(?<=\\s)EXT(?=\\s)|(?<=\\s)RC(?=\\s)|(?<=\\s)EMC(?=\\s)|(?<=\\s)FLOW(?=\\s)|(?<=\\s)DEPTH(?=\\s)|(?<=\\s)AREA(?=\\s)|(?<=\\s)DT(?=\\s)|(?<=\\s)HRT(?=\\s)|(?<=\\s)CONCEN(?=\\s)|(?<=\\s)MASS(?=\\s)|(?<=\\s)SHORT(?=\\s)|(?<=\\s)MEDIUM(?=\\s)|(?<=\\s)LONG(?=\\s)|(?<=\\s)STORAGE(?=\\s)|(?<=\\s)SHAPE(?=\\s)|(?<=\\s)DIVERSION(?=\\s)|(?<=\\s)TIDAL(?=\\s)|(?<=\\s)PUMP1(?=\\s)|(?<=\\s)PUMP2(?=\\s)|(?<=\\s)PUMP3(?=\\s)|(?<=\\s)PUMP4(?=\\s)|(?<=\\s)PUMP5(?=\\s)|(?<=\\s)RATING(?=\\s)|(?<=\\s)CONTROL(?=\\s)|(?<=\\s)FEET(?=\\s)|(?<=\\s)METERS(?=\\s)|(?<=\\s)DEGREES(?=\\s)|(?<=\\s)NONE(?=\\s)|(?<=\\s)MGD(?=\\s)|(?<=\\s)CFS(?=\\s)|(?<=\\s)LPS(?=\\s)|(?<=\\s)CMS(?=\\s)|(?<=\\s)MLD(?=\\s)|(?<=\\s)HORTON(?=\\s)|(?<=\\s)MODIFIED_GREEN_AMPT(?=\\s)|(?<=\\s)GREEN_AMPT(?=\\s)|(?<=\\s)CURVE_NUMBER(?=\\s)|(?<=\\s)STEADY(?=\\s)|(?<=\\s)KINWAVE(?=\\s)|(?<=\\s)DYNWAVE(?=\\s)|(?<=\\s)DEPTH(?=\\s)|(?<=\\s)ELEVATION(?=\\s)|(?<=\\s)H-W(?=\\s)|(?<=\\s)D-W(?=\\s)|(?<=\\s)PARTIAL(?=\\s)|(?<=\\s)FULL(?=\\s)|(?<=\\s)SLOPE(?=\\s)|(?<=\\s)FROUDE(?=\\s)|(?<=\\s)BOTH(?=\\s)"
    },
    "shapes":{
      "name":"string.shape.swmm",
      "match":"(?<=\\s)CIRCULAR(?=\\s)|(?<=\\s)FORCE_MAIN(?=\\s)|(?<=\\s)FILLED_CIRCULAR(?=\\s)|(?<=\\s)RECT_CLOSED(?=\\s)|(?<=\\s)RECT_OPEN(?=\\s)|(?<=\\s)TRAPEZOIDAL(?=\\s)|(?<=\\s)TRIANGULAR(?=\\s)|(?<=\\s)HORIZ_ELLIPSE(?=\\s)|(?<=\\s)VERT_ELLIPSE(?=\\s)|(?<=\\s)ARCH(?=\\s)|(?<=\\s)PARABOLIC(?=\\s)|(?<=\\s)POWER(?=\\s)|(?<=\\s)RECT_TRIANGULAR(?=\\s)|(?<=\\s)RECT_ROUND(?=\\s)|(?<=\\s)MODBASKETHANDLE(?=\\s)|(?<=\\s)EGG(?=\\s)|(?<=\\s)HORSESHOE(?=\\s)|(?<=\\s)GOTHIC(?=\\s)|(?<=\\s)CATENARY(?=\\s)|(?<=\\s)SEMIELLIPTICAL(?=\\s)|(?<=\\s)BASKETHANDLE(?=\\s)|(?<=\\s)SEMICIRCULAR(?=\\s)"
    },
    "options":{
      "name":"support.function.shape.swmm",
      "match":"^INPUT|^CONTINUITY|^FLOWSTATS|^CONTROLS|^SUBCATCHMENTS|^NODES|^LINKS|^CONSTANT|^MONTHLY|^TIMESERIES|^TEMPERATURE|^RECOVERY|^DRY_ONLY|^WINDSPEED|^SNOWMELT|^EVAPORATION|^RAINFALL|^CONDUCTIVITY|^FLOW_UNITS|^INFILTRATION|^FLOW_ROUTING|^LINK_OFFSETS|^FORCE_MAIN_EQUATION|^IGNORE_RAINFALL|^IGNORE_SNOWMELT|^IGNORE_GROUNDWATER|^IGNORE_RDII|^IGNORE_ROUTING|^IGNORE_QUALITY|^ALLOW_PONDING|^SKIP_STEADY_STATE|^SYS_FLOW_TOL|^LAT_FLOW_TOL|^START_DATE|^START_TIME|^END_DATE|^END_TIME|^REPORT_START_DATE|^REPORT_START_TIME|^SWEEP_START|^SWEEP_END|^DRY_DAYS|^REPORT_STEP|^WET_STEP|^DRY_STEP|^ROUTING_STEP|^LENGTHENING_STEP|^VARIABLE_STEP|^MINIMUM_STEP|^RULE_STEP|^INERTIAL_DAMPING|^NORMAL_FLOW_LIMITED|^MIN_SURFAREA|^MIN_SLOPE|^MAX_TRIALS|^HEAD_TOLERANCE|^THREADS|^TEMPDIR|^LID|^DIMENSIONS|^FILE|^AVERAGES"
    },
    "numerics":{
      "name":"constant.numeric.swmm",
      "match":"(?<=\\s)[+-]?([0-9]*[.])?[0-9]+(?=\\s)"
    },
    "dates":{
      "name":"constant.date.swmm",
      "match":"((\\d{1,2}(\\/|:)\\d{1,2}(\\/|:)\\d{2,4}))|(\\d{1,2}(\\/|:)\\d{1,2})"
    }


  },
  "scopeName": "source.inp"
}


